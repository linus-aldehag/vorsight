{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information",
      "Microsoft.Extensions.Hosting": "Information"
    }
  },
  "Server": {
    "Url": "http://localhost:3000",
    "PresharedKey": "your-secure-key"
  },
  "Agent": {
    "ExecutablePath": "Vorsight.Agent.exe",
    "TimeoutSeconds": 60,
    "AutoRestartOnCrash": true
  },
  "Monitoring": {
    "ScreenshotIntervalSeconds": 60,
    "ActivityPingIntervalSeconds": 30,
    "DefaultEnabled": true
  },
  "Storage": {
    "ScreenshotsPath": "C:\\ProgramData\\Vorsight\\screenshots",
    "LogsPath": "C:\\ProgramData\\Vorsight\\logs"
  }
}