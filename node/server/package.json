{
  "name": "@vorsight/server",
  "version": "1.6.0",
  "description": "",
  "main": "dist/server.js",
  "scripts": {
    "lint": "eslint . --ext .ts",
    "generate-types": "json2ts -i ../../shared/schemas/machine-settings.json -o src/types/settings.gen.ts && json2ts -i ../../shared/schemas/api-payloads.json -o src/types/payloads.gen.ts",
    "prebuild": "npm run generate-types",
    "start": "node dist/server.js",
    "dev": "npm run generate-types && tsx watch src/server.ts",
    "build": "tsc",
    "type-check": "tsc --noEmit",
    "build:web": "cd ..\\client && npm run build",
    "copy:web": "xcopy /E /I /Y ..\\client\\dist\\* public\\",
    "deploy:web": "npm run build:web && npm run copy:web",
    "all": "npm run deploy:web && npm run build && npm run dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^12.5.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "googleapis": "^144.0.0",
    "jsonwebtoken": "^9.0.3",
    "multer": "^2.0.2",
    "node-schedule": "^2.1.1",
    "ping": "^1.0.0",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.3",
    "@types/node-schedule": "^2.1.8",
    "json-schema-to-typescript": "^15.0.4",
    "nodemon": "^3.1.11",
    "prisma": "^5.22.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
